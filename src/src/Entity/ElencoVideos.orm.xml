<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\ElencoVideos">

        <many-to-many field="groups" target-entity="Group">

            <join-table name="elenco_videos">
                <join-columns>
                    <join-column name="cod_elenco" referenced-column-name="id_elenco" nullable="false" unique="false" />
                </join-columns>
                <inverse-join-columns>
                    <join-column name="cod_videos" referenced-column-name="id_videos" nullable="false" unique="false" />
                </inverse-join-columns>
            </join-table>
        </many-to-many>

    </entity>

</doctrine-mapping>
