<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\Videos">
        <id name="idVideos" column="id_videos" type="integer">
            <generator strategy="AUTO"/>
        </id>
        <field name="videTitulo" column="vide_titulo" type="string" length="255"/>
        <field name="videDescricao" column="videDescricao" type="string" length="255" nullable="true"/>
        <field name="videAno" column="vide_ano" type="string" length="255" nullable="true"/>
        <field name="videDuracao" column="vide_duracao" type="string" length="255" nullable="true"/>
        <field name="videImagemDiretorio" column="vide_imagem_diretorio" type="string" length="255" nullable="true"/>
        <field name="videTemporadaNumero" column="vide_temporada_numero" type="integer" length="255" nullable="true"/>
        <field name="createdAt" column="created_at" type="datetime"/>
        <field name="updatedAt" column="updated_at" type="datetime" nullable="true"/>
        <field name="deletedAt" column="deleted_at" type="datetime" nullable="true"/>
        <field name="createdBy" column="created_by" type="datetime"/>
        <field name="updatedBy" column="updated_by" type="datetime" nullable="true"/>
        <field name="deletedBy" column="deleted_by" type="datetime" nullable="true"/>

        <many-to-one field="codGenero" target-entity="Genero">
            <join-column name="cod_genero" referenced-column-name="id_genero" />
        </many-to-one>

        <many-to-one field="codVideoTipo" target-entity="VideoTipo">
            <join-column name="cod_video_tipo" referenced-column-name="id_video_tipo" />
        </many-to-one>

    </entity>

</doctrine-mapping>
